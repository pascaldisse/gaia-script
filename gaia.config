{
  "compiler": {
    "target": "web",
    "outputDir": "../build",
    "mainFile": "../main.gaia",
    "outputFileName": "gaia-compiled.js",
    "minify": false,
    "sourceMaps": true
  },
  "runtime": {
    "fallbacks": {
      "webgl": true,
      "audio": true,
      "localStorage": true,
      "fetch": true,
      "webWorker": true
    },
    "compatMode": "modern",
    "debugMode": true
  },
  "dependencies": {
    "useExternalUILibs": false,
    "bundleStyles": true,
    "includePolyfills": false
  }
}
